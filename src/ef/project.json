{
  "version": "1.0.0-*",
  "description": "Command-line tools for Entity Framework Core",
  "buildOptions": {
    "emitEntryPoint": true,
    "keyFile": "../../tools/Key.snk"
  },
  "configurations": {
    "Debug_x86": {
      "buildOptions": {
        "platform": "anycpu32bitpreferred",
        "define": [
          "DEBUG"
        ],
        "outputName": "ef.x86"
      }
    },
    "Release_x86": {
      "buildOptions": {
        "platform": "anycpu32bitpreferred",
        "outputName": "ef.x86"
      }
    }
  },
  "frameworks": {
    "net451": { },
    "netcoreapp1.0": {
      "dependencies": {
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.0.0"
        }
      }
    }
  }
}